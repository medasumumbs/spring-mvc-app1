    <path refid="library.maven:_org.slf4j:slf4j-api:2.0.7.classpath"/>
  </path>
  
  <path id="spring-mvc-app1.runtime.module.classpath">
    <pathelement location="${spring-mvc-app1.testoutput.dir}"/>
    <pathelement location="${spring-mvc-app1.output.dir}"/>
