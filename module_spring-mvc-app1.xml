  <patternset id="excluded.from.module.spring-mvc-app1">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.spring-mvc-app1">
    <patternset refid="excluded.from.module.spring-mvc-app1"/>
  </patternset>
  
  <path id="spring-mvc-app1.module.sourcepath">
    <dirset dir="${module.spring-mvc-app1.basedir}">
      <include name="src/main/resources"/>
      <include name="target/generated-sources/annotations"/>
    </dirset>
  </path>
  
  <path id="spring-mvc-app1.module.test.sourcepath">
    <dirset dir="${module.spring-mvc-app1.basedir}">
      <include name="src/main/java"/>
      <include name="src/main/java"/>
      <include name="target/generated-test-sources/test-annotations"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.spring-mvc-app1" depends="compile.module.spring-mvc-app1.production,compile.module.spring-mvc-app1.tests" description="Compile module spring-mvc-app1"/>
  
  <target name="compile.module.spring-mvc-app1.production" depends="register.custom.compilers" description="Compile module spring-mvc-app1; production classes">
    <mkdir dir="${spring-mvc-app1.output.dir}"/>
    <javac2 destdir="${spring-mvc-app1.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.spring-mvc-app1}/javac">
      <compilerarg line="${compiler.args.spring-mvc-app1}"/>
      <bootclasspath refid="spring-mvc-app1.module.bootclasspath"/>
      <classpath refid="spring-mvc-app1.module.production.classpath"/>
      <src refid="spring-mvc-app1.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.spring-mvc-app1"/>
    </javac2>
    
    <copy todir="${spring-mvc-app1.output.dir}">
      <fileset dir="${module.spring-mvc-app1.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.spring-mvc-app1.basedir}/target/generated-sources/annotations">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.spring-mvc-app1.tests" depends="register.custom.compilers,compile.module.spring-mvc-app1.production" description="compile module spring-mvc-app1; test classes" unless="skip.tests">
    <mkdir dir="${spring-mvc-app1.testoutput.dir}"/>
    <javac2 destdir="${spring-mvc-app1.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.spring-mvc-app1}/javac">
      <compilerarg line="${compiler.args.spring-mvc-app1}"/>
      <bootclasspath refid="spring-mvc-app1.module.bootclasspath"/>
      <classpath refid="spring-mvc-app1.module.classpath"/>
      <src refid="spring-mvc-app1.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.spring-mvc-app1"/>
    </javac2>
    
    <copy todir="${spring-mvc-app1.testoutput.dir}">
      <fileset dir="${module.spring-mvc-app1.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.spring-mvc-app1.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.spring-mvc-app1.basedir}/target/generated-test-sources/test-annotations">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.spring-mvc-app1" description="cleanup module">
    <delete dir="${spring-mvc-app1.output.dir}"/>
    <delete dir="${spring-mvc-app1.testoutput.dir}"/>
  </target>
